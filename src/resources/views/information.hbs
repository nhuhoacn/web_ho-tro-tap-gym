    <!-- Class Timetable Section End -->
    <h6 class="mb-4">lịch sử tập luyện</h6>
    <table class="table history table-dark">
        <thead>
            <tr>
                <th scope="col">Lớp</th>
                <th scope="col">Checkin</th>
                <th scope="col">Checkout</th>
                <th scope="col">Giá tập</th>
                <th scope="col">Trạng thái</th>
            </tr>
        </thead>
        <tbody>
            {{#each history}}
            <tr>
                <th scope="row">{{this.name}}</th>
                <td>{{this.registration_time}}</td>
                {{#ifCond this.cancellation_time "!=" "Invalid date"}}
                <td>{{this.cancellation_time}}</td>
                {{else}}
                <td>Tham gia</td>
                {{/ifCond}}
                <td>{{this.price}}</td>
                <td><button> Huỷ lớp</button></td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>